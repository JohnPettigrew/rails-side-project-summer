<% provide(:title, 'All projects') %>
<h1>All projects</h1>

<%= will_paginate :inner_window => 2, :outer_window => 0 %>

<ul class="index projects">
  <% @projects.each do |project| %>
    <%= render project %>
  <% end %>
</ul>

<%= will_paginate :inner_window => 2, :outer_window => 0 %>

<% if can? :create, Project %><p class="new-project"><%= link_to "List a new project", new_project_path %></p><% end %>
